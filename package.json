{
  "name": "investment-tracker",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . && npm run format:check",
    "lint:fix": "eslint . --fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "typeorm": "node --require tsconfig-paths/register --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/config/database/ormconfig-migration.ts",
    "typeorm:cli": "node --loader ts-node/esm ./node_modules/typeorm/cli.js -d ./src/config/database/ormconfig-migration.ts",
    "env:load": "env-cmd",
    "migration-generate": "npm run typeorm:cli -- migration:generate -d ./src/config/typeorm.ts",
    "migration:run": "env-cmd npm run typeorm -- migration:run -- -d ./src/config/database/ormconfig-migration.ts",
    "migration:show": "env-cmd npm run typeorm:cli -- migration:show -- -d ./src/config/database/ormconfig-migration.ts",
    "migration:generate": "env-cmd npm run typeorm -- migration:generate ./src/migrations/new -- -d ./src/config/database/ormconfig-migration.ts",
    "migration:create": "npm run typeorm:cli -- migration:create",
    "migration:revert": "npm run typeorm -- -d ./src/config/typeorm.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/typeorm": "10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "multer": "^1.4.4",
    "papaparse": "^5.4.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.7",
    "@types/jest-when": "^3.5.4",
    "@types/node": "^16.18.32",
    "@types/papaparse": "^5.3.7",
    "@types/passport-jwt": "^3.0.12",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-when": "^3.6.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  }
}
